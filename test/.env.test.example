# Test Environment Configuration
# Copy this to .env.test and fill in your actual values

# Alchemy API key for forking Arbitrum mainnet
# Get your key at: https://www.alchemy.com/
ALCHEMY_API_KEY=your_alchemy_api_key_here

# CoinGecko API key for price service tests
# Get your key at: https://www.coingecko.com/en/api
COINGECKO_API_KEY=CG-your_coingecko_api_key_here

# TheGraph API key for subgraph queries
# Get your key at: https://thegraph.com/studio/
THEGRAPH_API_KEY=your_thegraph_api_key_here

# Test configuration
QUIET_TESTS=false
NODE_ENV=test

# Optional: Override default test settings
# GANACHE_PORT=8545
# GANACHE_BLOCK_TIME=0.5
# TEST_TIMEOUT=30000
