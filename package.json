{
  "name": "fum_library",
  "version": "1.0.0",
  "author": "Derrek Brack & Claude Code",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "Shared utilities for FUM DeFi management",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./configs": "./dist/configs/index.js",
    "./adapters": "./dist/adapters/index.js",
    "./helpers": "./dist/helpers/index.js",
    "./helpers/formatHelpers": "./dist/helpers/formatHelpers.js",
    "./helpers/chainHelpers": "./dist/helpers/chainHelpers.js",
    "./helpers/tokenHelpers": "./dist/helpers/tokenHelpers.js",
    "./helpers/platformHelpers": "./dist/helpers/platformHelpers.js",
    "./helpers/strategyHelpers": "./dist/helpers/strategyHelpers.js",
    "./helpers/vaultHelpers": "./dist/helpers/vaultHelpers.js",
    "./blockchain": "./dist/blockchain/index.js",
    "./blockchain/wallet": "./dist/blockchain/wallet.js",
    "./blockchain/contracts": "./dist/blockchain/contracts.js",
    "./services": "./dist/services/index.js",
    "./services/coingecko": "./dist/services/coingecko.js",
    "./artifacts": "./dist/artifacts/index.js",
    "./artifacts/contracts": "./dist/artifacts/contracts.js",
    "./artifacts/contracts.js": "./dist/artifacts/contracts.js",
    "./test/setup/ganache-config": "./test/setup/ganache-config.js",
    "./test/setup/test-contracts": "./test/setup/test-contracts.js",
    "./test/test-env": "./test/test-env.js"
  },
  "files": [
    "dist",
    "test/setup",
    "test/test-env.js",
    "bytecode"
  ],
  "scripts": {
    "build": "rm -rf dist && mkdir -p dist && cp -r src/* dist/ && npm run docs",
    "docs": "node scripts/generate-docs.js",
    "prepublishOnly": "npm run build",
    "pack": "npm run build && npm pack && (cd ../fum_automation && npm uninstall fum_library 2>/dev/null || true) && (cd ../fum_automation && npm install file:../fum_library/fum_library-1.0.0.tgz) && (cd ../fum && npm uninstall fum_library 2>/dev/null || true) && (cd ../fum && npm install file:../fum_library/fum_library-1.0.0.tgz --legacy-peer-deps --force)",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "node scripts/test-integration.js",
    "test:unit": "vitest run unit"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "@uniswap/sdk-core": "^5.9.0",
    "@uniswap/smart-order-router": "^4.7.7",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.18.2",
    "jsbi": "^3.2.5",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "chai": "^5.2.0",
    "dotenv": "^16.5.0",
    "ethers": "^5.7.2",
    "ganache": "^7.9.2",
    "sinon": "^20.0.0",
    "vitest": "^3.1.3"
  }
}
